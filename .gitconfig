# This is Git's per-user configuration file.
[core]
    editor = vim
    excludesfile = /Users/t/.gitignore_global
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
[user]
	email = t@ineeza.com
	name = Tetsuro Takemoto
[init]
	defaultBranch = main
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
